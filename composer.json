{
  "name": "the-coding-owl/owl-cal",
  "description": "A calendar app",
  "type": "typo3-cms-extension",
  "require": {
    "typo3/cms-core": "^11.5.1",
    "typo3/cms-extbase": "^11.5.1",
    "typo3/cms-fluid": "^11.5.1"
  },
  "keywords": ["typo3", "calendar"],
  "homepage": "https://github.com/the-coding-owl/owl-cal",
  "support": {
    "issues": "https://github.com/the-coding-owl/owl-cal/issues",
    "source": "https://github.com/the-coding-owl/owl-cal"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.0.1",
    "saschaegerer/phpstan-typo3": "^1.0.0",
    "phpstan/extension-installer": "^1.1.0",
    "squizlabs/php_codesniffer": "^3.6.1",
    "roave/security-advisories": "dev-master",
    "friendsofphp/php-cs-fixer": "^3.2"
  },
  "license": "GPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "TheCodingOwl\\OwlCal\\": "Classes/"
    }
  },
  "authors": [
    {
      "name": "Kevin Ditscheid",
      "email": "kevin@the-coding-owl.de",
      "role": "owner"
    }
  ],
  "extra": {
    "typo3/cms": {
      "extension-key": "owl_cal"
    }
  }
}
